package Q2;

import java.util.Scanner;

public class App {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		ContaBancaria cb = new ContaBancaria();
		
		
		
		//Numero
		
		System.out.println("Digite o número: "+cb.getNumero());
		cb.setNumero(null);
		
		//Titular
		System.out.println("Digite o títular"+ cb.getTitular());
		cb.setTitular("Carlos");
		
		//Saldo
		System.out.println("Digite o saldo: "+cb.getSaldo());
		cb.setSaldo(1000);
		
		//Depositar
		System.out.println("Informe o valor a depositar: ");
		cb.depositar(500);
	}

}
