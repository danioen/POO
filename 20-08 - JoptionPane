package exemploJoptionPane;

import javax.swing.JOptionPane;

public class main {

	public static void main(String[] args) {
		JOptionPane.showMessageDialog(null,"Ollhe para os dois lados antes de atravessar","Atenção",
		JOptionPane.WARNING_MESSAGE);
		
		String nome = JOptionPane.showInputDialog(null, "Qual é o seu nome? ");
		JOptionPane.showMessageDialog(null,"Oi "+nome+" !!!");
;	}

}
